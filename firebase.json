{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    }
  },
  "apphosting": {
    "build": {
      "command": "npm run build"
    }
  },
  "firestore": {
    "rules": "firestore.rules"
  }
}
