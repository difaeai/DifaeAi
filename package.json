{
  "name": "camera-bridge-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm --workspace frontend run dev",
    "build": "npm --workspace frontend run build",
    "start": "npm --workspace frontend run start",
    "lint": "npm --workspace frontend run lint",
    "postinstall": "patch-package || true",
    "format": "prettier --write \"frontend/**/*.{ts,tsx,js,json,md}\""
  },
  "workspaces": [
    "frontend"
  ],
  "dependencies": {
    "hls.js": "^1.5.8"
  },
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "patch-package": "^8.0.1"
  },
  "packageManager": "npm@10.8.2"
}
